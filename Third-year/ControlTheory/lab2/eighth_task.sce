function [zr]=G(c,z)
    zr = z(2) - (1 / (c(1)+c(2)*%e^(-z(1))))
endfunction

//Исходные данные
U = [0 1 1.5 2 2.5 3 3.5 4 4.5 5];
V = [12 10.1 11.58 17.4 30.68 53.6 87.78 136.9 202.5 287];
//Формирование матрицы исходных данных
z = [U;V];

//Вектор начальных приближений
c = [1; 1];
//Решение задачи 
[a, err]=datafit(G, z, c);

printf("Решение: ");
disp(a);

printf("\nОшибка: ");
disp(err);
// 1/(A + B*E^(-U))
plot2d(U,V, -4);
t = 0:0.1:5;
v = 1 ./ (a(1)+a(2).*%e^(-t));
plot2d(t,v);